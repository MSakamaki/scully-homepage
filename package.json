{
  "name": "web-page",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "update:npm": "npx ncu \"/^(npm-check-updates|@scullyio|concurrently|apollo-angular|firebase-tools|husky|@gammastream/scully-plugin-sitemap|fast-xml-parser|xmlbuilder|post-merge-install|cypress-image-snapshot)/\"",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "scully": "scully",
    "scully:serve": "scully serve",
    "homepage:serve": "ng run homepage:serve",
    "api:serve": "ng run api:serve",
    "fb:serve": "firebase serve",
    "preinstall": "npx check-node-version --node 14",
    "prestart": "npx check-node-version --node 14",
    "pretest": "npx check-node-version --node 14",
    "serve:lhci": "npm run scully:serve",
    "workspace-generator": "nx workspace-generator"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/fire": "^6.0.3",
    "@angular/forms": "11.0.5",
    "@angular/platform-browser": "11.0.5",
    "@angular/platform-browser-dynamic": "11.0.5",
    "@angular/router": "11.0.5",
    "@apollo/client": "^3.0.0",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nrwl/angular": "11.0.15",
    "@scullyio/init": "^1.0.1",
    "@scullyio/ng-lib": "^1.0.0",
    "@scullyio/scully": "^1.0.0",
    "apollo-angular": "^2.0.4",
    "firebase-admin": "^9.2.0",
    "firebase-functions": "^3.11.0",
    "graphql": "^15.0.0",
    "ngx-markdown": "^10.1.1",
    "normalize.css": "^8.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": ">= 0.900 < 0.1100",
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular/cli": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@gammastream/scully-plugin-sitemap": "^1.0.4",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "11.0.15",
    "@nrwl/cypress": "11.0.15",
    "@nrwl/jest": "11.0.15",
    "@nrwl/nest": "11.0.15",
    "@nrwl/node": "11.0.15",
    "@nrwl/tao": "11.0.15",
    "@nrwl/workspace": "11.0.15",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "codelyzer": "6.0.1",
    "concurrently": "^5.3.0",
    "cypress": "^4.1.0",
    "cypress-image-snapshot": "^3.1.1",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "fast-xml-parser": "^3.17.4",
    "firebase-tools": "^8.0.0",
    "fuzzy": "^0.1.3",
    "husky": "^4.3.0",
    "inquirer": "^6.2.2",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "jest": "26.2.2",
    "jest-preset-angular": "8.3.1",
    "npm-check-updates": "^9.1.2",
    "open": "^7.0.3",
    "post-merge-install": "^0.4.0",
    "prettier": "2.1.2",
    "ts-jest": "26.4.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "typescript": "~4.0.3",
    "xmlbuilder": "^15.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:write && npm run affected:lint -- --parallel --uncommitted && npm run affected:test -- --parallel --uncommitted && git add .",
      "pre-push": "docker-compose -f .docker/e2e/docker-compose.yml up --build && npx nx run-many --target=build --configuration=production --projects=homepage,api --parallel && npm run scully",
      "post-merge": "post-merge-install",
      "post-rebase": "post-merge-install"
    }
  },
  "main": "dist/apps/api/main.js",
  "engines": {
    "node": "14"
  }
}
